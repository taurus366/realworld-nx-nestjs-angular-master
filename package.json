    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "migration:create": "./node_modules/.bin/typeorm migration:create -n",
    "migration:run": "npx tsc migrations/*.ts --outDir migrations && ./node_modules/.bin/typeorm migration:run",
    "serve:conduit": "nx serve conduit --port 4200",
    "serve:api": "nx serve api",
    "serve:api-conduit": "concurrently -p=\"{name}\" -n=\"Conduit,API\" -c=\"blue,yellow\" \"npm run serve:conduit\" \"npm run serve:api\"",
    "build-prod:conduit": "nx build conduit --prod",
    "build-prod:api": "nx build api --prod"
